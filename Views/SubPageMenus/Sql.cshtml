@model IEnumerable<TrojWebApp.Models.SubPageMenusViewModel>

@{
    ViewData["Title"] = "Insert into SubPageMenus";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>@ViewData["Title"]</h1>
<table class="table table-condensed table-no-border">

        @foreach (var item in Model)
        {
            <tr>
                <td class="tablefont2" nowrap>
                INSERT INTO SubPageMenus (ParentPageId,ChildPageId,Position,Changed,ChangedBy) VALUES(@Html.DisplayFor(modelItem => item.ParentPageId),@Html.DisplayFor(modelItem => item.ChildPageId),@Html.DisplayFor(modelItem => item.Position),'@Html.DisplayFor(modelItem => item.Changed)','@Html.DisplayFor(modelItem => item.ChangedBy)');
                </td>
            </tr>
        }
</table>
